{
  "name": "dify-cron",
  "version": "1.0.0",
  "description": "Dify API integration with Google Sheets using Google Apps Script",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "push": "clasp push",
    "deploy": "npm run build && npm run push",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write 'src/**/*.ts' 'tests/**/*.ts'",
    "clasp:login": "clasp login",
    "clasp:create": "clasp create --title \"Dify Apps Sheet\" --type standalone",
    "clasp:push": "clasp push",
    "clasp:deploy": "clasp deploy"
  },
  "keywords": [
    "dify",
    "google-apps-script",
    "automation"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@google/clasp": "^2.4.2",
    "@types/google-apps-script": "^1.0.78",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.24",
    "@typescript-eslint/eslint-plugin": "^7.1.0",
    "@typescript-eslint/parser": "^7.1.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "prettier": "^3.2.5",
    "ts-jest": "^29.1.2",
    "typescript": "^5.3.3"
  }
}
